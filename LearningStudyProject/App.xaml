    <Application x:Class="LearningStudyProject.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LearningStudyProject"
             xmlns:views="clr-namespace:LearningStudyProject.Views"
             xmlns:viewmodels="clr-namespace:LearningStudyProject.ViewModels"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewmodels:SubjectsTreeViewModel}">
            <views:SubjectTreeView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:QACreateEditViewModel}">
            <views:QACreateEditView/>
        </DataTemplate>
        

    </Application.Resources>
</Application>
